{
  "name": "gutenberg-collaborative-editing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "wp-scripts build",
    "format": "wp-scripts format",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "wp-scripts lint-js",
    "lint:css": "wp-scripts lint-style",
    "lint:js:fix": "wp-scripts lint-js --fix",
    "lint:css:fix": "wp-scripts lint-style --fix",
    "start": "wp-scripts start",
    "packages-update": "wp-scripts packages-update",
    "package": "./scripts/package.sh",
    "wp-env": "wp-env",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "wp-scripts lint-js --fix",
    "*.{css,scss}": "wp-scripts lint-style --fix",
    "*.{ts,tsx}": "./scripts/type-check-staged.sh"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/wordpress__block-editor": "^11.5.17",
    "@types/wordpress__blocks": "^12.5.18",
    "@types/wordpress__components": "^23.0.12",
    "@types/wordpress__core-data": "^2.4.5",
    "@types/wordpress__data": "^6.0.2",
    "@types/wordpress__editor": "^14.3.1",
    "@types/wordpress__element": "^2.4.1",
    "@types/wordpress__i18n": "^3.4.0",
    "@types/wordpress__plugins": "^3.0.3",
    "@wordpress/blocks": "^15.2.0",
    "@wordpress/components": "^30.2.0",
    "@wordpress/data": "^10.29.0",
    "@wordpress/dom-ready": "^4.29.0",
    "@wordpress/element": "^6.29.0",
    "@wordpress/env": "^10.29.0",
    "@wordpress/i18n": "^6.2.0",
    "@wordpress/plugins": "^7.29.0",
    "@wordpress/scripts": "^30.22.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "ts-jest": "^29.2.3",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
